{
  "name": "@dcyfr/ai-notebooks",
  "version": "1.0.1",
  "description": "Data science notebook toolkit with TypeScript tooling for Jupyter notebook management, data pipelines, and visualization",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./notebook": "./src/notebook/index.ts",
    "./pipeline": "./src/pipeline/index.ts",
    "./visualization": "./src/visualization/index.ts",
    "./utils": "./src/utils/index.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "example:explore": "tsx examples/data-exploration/index.ts",
    "example:pipeline": "tsx examples/data-pipeline/index.ts",
    "example:analysis": "tsx examples/model-analysis/index.ts",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "dcyfr",
    "ai",
    "jupyter",
    "notebooks",
    "data-science",
    "data-pipeline",
    "visualization",
    "typescript"
  ],
  "author": "DCYFR <hello@dcyfr.ai>",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.10",
    "@eslint/js": "^9.20.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.20.0",
    "tsx": "^4.19.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^4.0.18"
  }
}
